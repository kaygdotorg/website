{
  "name": "personal-page",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "node scripts/copy-content-assets.mjs && node scripts/generate-page-manifest.mjs && astro dev",
    "build": "node scripts/copy-content-assets.mjs && node scripts/generate-page-manifest.mjs && astro build && pagefind --site dist",
    "build:search": "node scripts/copy-content-assets.mjs && node scripts/generate-page-manifest.mjs && astro build && pagefind --site dist --output-path public/pagefind",
    "generate-manifest": "node scripts/generate-page-manifest.mjs",
    "preview": "astro preview",
    "astro": "astro",
    "sync": "node scripts/sync-obsidian.mjs",
    "copy-assets": "node scripts/copy-content-assets.mjs"
  },
  "dependencies": {
    "@astrojs/sitemap": "^3.7.0",
    "@fontsource/cascadia-code": "^5.2.3",
    "@fontsource/mrs-saint-delafield": "^5.2.5",
    "@fontsource/outfit": "^5.2.5",
    "@fontsource/playfair-display": "^5.2.8",
    "@fontsource/sn-pro": "^5.2.5",
    "@pagefind/default-ui": "^1.4.0",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.16.14",
    "daisyui": "^5.5.14",
    "dotenv": "^17.2.3",
    "gray-matter": "^4.0.3",
    "hast-util-from-html": "^2.0.3",
    "hast-util-to-string": "^3.0.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "tailwindcss": "^4.1.18",
    "turndown": "^7.2.2",
    "unist-util-visit": "^5.1.0"
  },
  "devDependencies": {
    "pagefind": "^1.4.0"
  }
}
